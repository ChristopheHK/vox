{
    "functions": {
        "api/proxy.js": {
            "maxDuration": 30
        }
    },
    "headers": [
        {
            "source": "/api/(.*)",
            "headers": [
                {
                    "key": "X-Frame-Options",
                    "value": "ALLOWALL"
                },
                {
                    "key": "Content-Security-Policy",
                    "value": "frame-ancestors *"
                }
            ]
        }
    ]
}
